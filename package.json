{
	"name": "terminaltab",
	"displayName": "TerminalTab",
	"description": "A extension that allow to to place terminals anywhere",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:terminaltab.helloWorld",
		"onCommand:terminaltab.createTerminal"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "terminaltab.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "terminaltab.createTerminal",
				"title": "Launch a terminal tab"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"xterm": "^4.8.1"
	}
}
