{
    "name": "terminaltab",
    "displayName": "TerminalTab",
    "description": "A extension that allow to to place terminals anywhere",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.48.0"
    },
    "publisher": "mmis1000-personal",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:terminaltab.createTerminal",
        "onWebviewPanel:terminalTab",
        "onWebviewPanel:terminalTab.terminal"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "terminaltab.createTerminal",
                "title": "Launch a terminal tab here",
                "icon": "$(terminal)"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "terminaltab.createTerminal",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "command": "terminaltab.createTerminal"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^14.0.27",
        "@types/vscode": "^1.48.0",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "eslint": "^7.6.0",
        "glob": "^7.1.6",
        "mocha": "^8.0.1",
        "typescript": "^3.8.3",
        "node-pty": "^0.9.0",
        "vscode-test": "^1.4.0"
    },
    "dependencies": {
        "xterm": "^4.8.1",
        "xterm-addon-fit": "^0.4.0",
        "xterm-addon-serialize": "^0.3.0",
        "xterm-addon-unicode11": "^0.2.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mmis1000/Vscode-terminal-tab.git"
    },
    "extensionKind": [
        "workspace"
    ]
}
